    <style name="Theme.PfeDomotique.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.PfeDomotique.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>